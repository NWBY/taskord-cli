{
    "name": "taskord-cli",
    "description": "Create tasks from your terminal",
    "version": "0.0.0",
    "author": "NWBY @NWBY",
    "bin": {
        "taskord": "./bin/run"
    },
    "bugs": "https://github.com/NWBY/taskord-cli/issues",
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@types/node": "^14.14.31",
        "globby": "^10.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/NWBY/taskord-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "taskord",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "NWBY/taskord-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
        "test": "echo NO TESTS",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.0",
        "@types/fs-extra": "^9.0.11",
        "axios": "^0.21.1",
        "cli-ux": "^5.5.1",
        "fs-extra": "^8.1.0",
        "tslib": "^1.14.1"
    }
}
